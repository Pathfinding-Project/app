{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start:dev": "concurrently \"cd client && npm start\" \"cd server && npm start\"",
    "start": "cd server && node --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only src/index.ts",
    "start:all": "concurrently \"cd client && npx --yes serve -s build\" \"cd server && ts-node src/index.ts\"",
    "build:all": "cd client && npm run build",
    "deploy:all": "gh-pages -d client/build",
    "postinstall": "concurrently \"cd client && npm i\" \"cd server && npm i\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pathfinding-Project/app.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Pathfinding-Project/app/issues"
  },
  "homepage": "https://pathfinding-project.github.io/app",
  "devDependencies": {
    "concurrently": "^6.2.1",
    "gh-pages": "^3.2.3"
  },
  "dependencies": {
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  }
}

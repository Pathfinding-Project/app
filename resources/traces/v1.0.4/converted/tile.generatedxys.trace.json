          "fill": "0x000000",
          "x": "{{ctx.tile_x+ctx.width/6}}",
          "y": "{{ctx.tile_y-ctx.height/4}}"
        },
        "fill": "{{ctx[`${ctx.tile_x}${ctx.tile_y}`] ? 0x808080 : 0xFFFFFFF}}",
        "persist": false,
        "x": "{{ctx.tile_x}}",
        "y": "{{ctx.tile_y}}"
      }
    ],
    "tilerow": [
